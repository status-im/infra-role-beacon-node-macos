<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>homebrew.mxcl.logrotate</string>
  <key>ProgramArguments</key>
  <array>
	  <string>{{ homebrew_path }}/opt/logrotate/sbin/logrotate</string>
	  <string>{{ beacon_node_logrotate_main_config }}</string>
  </array>
  <key>RunAtLoad</key>
  <false/>
  <key>StartInterval</key>
  <integer>{{ beacon_node_logrotate_service_interval_sec }}</integer>
</dict>
</plist>
